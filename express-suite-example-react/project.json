{
  "name": "express-suite-example-react",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "express-suite-example-react/src",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project express-suite-example-react --web",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "outputs": [
        "{workspaceRoot}/dist/express-suite-example-react"
      ],
      "options": {
        "cwd": "express-suite-example-react",
        "command": "vite build --mode development"
      },
      "configurations": {
        "development": {
          "command": "vite build --mode development"
        },
        "production": {
          "command": "vite build --mode production"
        }
      }
    }
  }
}
