{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug express-suite-example-api with Nx",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["nx", "serve", "express-suite-example-api"],
      "env": {
        "NODE_OPTIONS": "--inspect=9229"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/express-suite-example-api/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug express-suite-example-inituserdb with Nx",
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["nx", "serve", "express-suite-example-inituserdb"],
      "env": {
        "NODE_OPTIONS": "--inspect=9230"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/express-suite-example-inituserdb/dist/**/*.(m|c|)js",
        "!**/node_modules/**"
      ]
    }
  ]
}
