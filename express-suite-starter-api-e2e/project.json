{
  "name": "express-suite-starter-api-e2e",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["express-suite-starter-api"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "express-suite-starter-api-e2e/jest.config.cts",
        "passWithNoTests": true
      },
      "dependsOn": [
        "express-suite-starter-api:build",
        "express-suite-starter-api:serve"
      ]
    }
  }
}
