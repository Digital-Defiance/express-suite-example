  "scripts": {
    "build": "NODE_ENV=production npx nx run-many --target=build --all --configuration=production",
    "build:dev": "NODE_ENV=development npx nx run-many --target=build --all --configuration=development",
    "test:all": "yarn test:jest && yarn test:e2e",
    "test:jest": "NODE_ENV=development npx nx run-many --target=test --all --configuration=development",
    "lint:all": "npx nx run-many --target=lint --all",
    "prettier:check": "prettier --check '**/*.{ts,tsx}'",
    "prettier:fix": "prettier --write '**/*.{ts,tsx}'",
    "serve": "npx nx serve express-suite-example-api --configuration production",
    "serve:stream": "npx nx serve express-suite-example-api --configuration production --output-style=stream",
    "serve:dev": "npx nx serve express-suite-example-api --configuration development",
    "serve:dev:stream": "npx nx serve express-suite-example-api --configuration development --output-style=stream",
    "build:api": "npx nx build express-suite-example-api",
    "build:react": "npx nx build express-suite-example-react",
    "inituserdb": "yarn build:dev && npx nx serve express-suite-example-inituserdb --output-style=stream",
    "inituserdb:drop": "yarn build:dev && npx nx serve express-suite-example-inituserdb --output-style=stream --args=--drop",
    "inituserdb:setenv": "yarn build:dev && npx nx serve express-suite-example-inituserdb --output-style=stream --args=--setEnv",
    "inituserdb:drop:setenv": "yarn build:dev && npx nx serve express-suite-example-inituserdb --output-style=stream --args=\"--drop --setEnv\""
  }
